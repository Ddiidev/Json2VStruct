module scripts_gen

import x.json2

// Struct to be generated
@struct_gen

const json = r'
{
	"name": "André",
	"age": 25,
	"is_people": true,
	"height": 1.75
}
'

fn test_simple_keys() {
	mut obj_analyzed := json2.decode[Root](json)!

	assert obj_analyzed.name == "André"
	assert obj_analyzed.age == 25
	assert obj_analyzed.is_people == true
	assert obj_analyzed.height == 1.75
}
